generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model TestLog {
  id        Int      @id @default(autoincrement())
  message   String
  createdAt DateTime @default(now())
}

model Member {
  id        Int       @id @default(autoincrement())
  studentId String    @db.VarChar(20)
  fullname  String    @db.VarChar(100)
  createdAt DateTime? @default(now()) @db.Timestamp(0)
}
